{
  "name": "@mause/www-sacred",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mause/www-sacred.git"
  },
  "description": "SRCL is an open-source React component and style repository that helps you build web applications, desktop applications, and static websites with terminal aesthetics.",
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "version": "1.1.17",
  "type": "module",
  "files": [
    "package.json",
    "./dist/srcl.js",
    "./dist/srcl.d.ts",
    "global.css",
    "global-fonts.css",
    "./global.css",
    "./global-fonts.css",
    "./dist/components.js",
    "./dist/components.d.ts",
    "dist"
  ],
  "exports": {
    "./global.css": {
      "import": "./global.css"
    },
    "./global-fonts.css": {
      "import": "./global-fonts.css"
    },
    "./www-sacred.css": {
      "import": "./dist/www-sacred.css"
    },
    ".": {
      "types": "./dist/srcl.d.ts",
      "import": "./dist/srcl.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    }
  },
  "scripts": {
    "dev": "next -p 10000",
    "build": "rimraf dist && vite build",
    "check-exports": "yarn build && npx publint",
    "start": "PORT=10000 next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "^16.0.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "rimraf": "^6.1.2",
    "rollup-preserve-directives": "^1.1.3",
    "sass": "1.94.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "validate-package-exports": "^0.14.0",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "yarn@4.12.0"
}
